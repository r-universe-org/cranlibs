CONFIG_VERSION=3.5.3
CONFIG_INST_PREFIX=/opt/R/x86_64
CONFIG_INST_LIBS=-L/opt/R/x86_64/lib -lgdal
CONFIG_INST_CFLAGS=-I/opt/R/x86_64/include
CONFIG_INST_DATA=/opt/R/x86_64/share/gdal
prefix=${CONFIG_INST_PREFIX}
