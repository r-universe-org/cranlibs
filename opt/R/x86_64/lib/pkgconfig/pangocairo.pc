prefix=/opt/R/x86_64
includedir=${prefix}/include
libdir=${prefix}/lib

Name: Pango Cairo
Description: Cairo rendering support for Pango
Version: 1.50.14
Requires: pango, pangoft2, glib-2.0 >=  2.62, gobject-2.0 >=  2.62, gio-2.0 >=  2.62, fribidi >=  1.0.6, harfbuzz >=  2.6.0, fontconfig >=  2.13.0, freetype2, cairo >=  1.12.10
Libs: -L${libdir} -lpangocairo-1.0 -lm -framework CoreFoundation -framework ApplicationServices
Cflags: -I${includedir}/pango-1.0
